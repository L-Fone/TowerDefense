<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AssemblyName>DCET.Runtime</AssemblyName>
    <RootNamespace>DCET</RootNamespace>
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;SERVER</DefineConstants>
    <OutputPath></OutputPath>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;RELEASE;SERVER</DefineConstants>
    <OutputPath>..\..\..\Bin\</OutputPath>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Libs\**" />
    <EmbeddedResource Remove="Libs\**" />
    <None Remove="Libs\**" />
    <None Remove="graph.bytes" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\Unity\Assets\Hotfix\Message\AutoGeneratedCode\OuterMessage.cs" Link="Message\AutoGeneratedCode\OuterMessage.cs" />
    <Compile Include="..\..\..\Unity\Assets\Hotfix\Message\AutoGeneratedCode\OuterOpcode.cs" Link="Message\AutoGeneratedCode\OuterOpcode.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Packages\DCET.BehaviorTree\DCET.BehaviorTree.csproj" />
    <ProjectReference Include="..\..\Packages\DCET.Config\DCET.Config.csproj" />
    <ProjectReference Include="..\..\Packages\DCET.Core\DCET.Core.csproj" />
    <ProjectReference Include="..\..\Packages\DCET.Message\DCET.Message.csproj" />
    <ProjectReference Include="..\..\Packages\DCET.Pathfinding\DCET.Pathfinding.csproj" />
    <ProjectReference Include="..\..\Packages\Protobuf\Protobuf.csproj" />
  </ItemGroup>
</Project>